              data-transition-enter-active="ease-in-out duration-200 sm:duration-400"
              data-transition-enter-from="translate-x-full"
              data-transition-enter-to="translate-x-0"
              data-transition-leave-active="ease-in-out duration-200 sm:duration-400"
              data-transition-leave-from="translate-x-0"
              data-transition-leave-to="translate-x-full"
              data-action="transition:end-leave-&gt;ui-kit--drawer--default-component#resetTransition"
            >
