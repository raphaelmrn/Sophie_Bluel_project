      data-ui-kit--drawer--default-component-focus-value="true"
    >
      <div
        class="fixed inset-0 bg-gray-500 bg-opacity-75 transition-opacity opacity-0 hidden"
        data-ui-kit--drawer--default-component-target="backgroundBackdrop"
        data-controller="transition"
        data-transition-enter-active="ease-in-out duration-200"
        data-transition-enter-from="opacity-0"
        data-transition-enter-to="opacity-100"
        data-transition-leave-active="ease-in-out duration-200"
        data-transition-leave-from="opacity-100"
        data-transition-leave-to="opacity-0"
        data-action="transition:end-leave-&gt;ui-kit--drawer--default-component#resetTransition"
      ></div>

      <div class="fixed inset-0 overflow-hidden">
        <div class="absolute inset-0 overflow-hidden">
          <div
            class="pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10"
